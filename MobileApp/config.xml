<?xml version='1.0' encoding='utf-8'?>
<widget id="technology.dean" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>MALBT</name>
    <description>Final Year Assignment</description>
    <author email="MATLB@dean.technology" href="http://dean.technology/">Dean Meehan</author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <platform name="android">
        <preference name="android-minSdkVersion" value="14" />
        <icon density="ldpi" src="www/img/eventMap/evLogoLogin.png" />
        <icon density="mdpi" src="www/img/eventMap/evLogoLogin.png" />
        <icon density="hdpi" src="www/img/eventMap/evLogoLogin.png" />
        <icon density="xhdpi" src="www/img/eventMap/evLogoLogin.png" />
        <icon density="xxhdpi" src="www/img/eventMap/evLogoLogin.png" />
        <icon density="xxxhdpi" src="www/img/eventMap/evLogoLogin.png" />
        <splash density="land-ldpi" src="www/img/eventMap/evLogoLogin.png" />
        <splash density="land-mdpi" src="www/img/eventMap/evLogoLogin.png" />
        <splash density="land-hdpi" src="www/img/eventMap/evLogoLogin.png" />
        <splash density="land-xhdpi" src="www/img/eventMap/evLogoLogin.png" />
        <splash density="land-xxhdpi" src="www/img/eventMap/evLogoLogin.png" />
        <splash density="land-xxxhdpi" src="www/img/eventMap/evLogoLogin.png" />
        <splash density="port-ldpi" src="www/img/eventMap/evLogoLogin.png" />
        <splash density="port-mdpi" src="www/img/eventMap/evLogoLogin.png" />
        <splash density="port-hdpi" src="www/img/eventMap/evLogoLogin.png" />
        <splash density="port-xhdpi" src="www/img/eventMap/evLogoLogin.png" />
        <splash density="port-xxhdpi" src="www/img/eventMap/evLogoLogin.png" />
        <splash density="port-xxxhdpi" src="www/img/eventMap/evLogoLogin.png" />
        <access origin="*" />
        <allow-intent href="*" />
        <allow-navigation href="*" />
    </platform>
    <plugin name="cordova-plugin-media-capture" source="npm" spec="~1.2.0" />
    <plugin name="cordova-plugin-device" source="npm" spec="~1.1.1" />
    <plugin name="cordova-plugin-dialogs" source="npm" spec="~1.2.0" />
    <plugin name="cordova-plugin-file" source="npm" spec="~4.1.1" />
    <plugin name="cordova-plugin-file-transfer" source="npm" spec="~1.5.0" />
    <plugin name="cordova-plugin-geolocation" source="npm" spec="~2.1.0" />
    <plugin name="cordova-plugin-media" source="npm" spec="~2.2.0" />
    <plugin name="cordova-plugin-eddystone" spec="cordova-plugin-eddystone" />
    <plugin name="cordova-plugin-whitelist" spec="~1.3.1" />
    <plugin name="com.red_folder.phonegap.plugin.backgroundservice" spec="https://github.com/Red-Folder/bgs-core.git" />
    <plugin name="technology.dean.backgroundservice" spec=".\bgs-plugin" />
    <engine name="android" spec="~6.0.0" />
</widget>
